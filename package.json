    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-exports": "^0.7.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.1.2",
    "nodejs-file-downloader": "^4.10.2",
    "parcel": "^2.8.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "antlr4": "^4.11.0"
